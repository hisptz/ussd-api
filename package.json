{
  "name": "boilerplate-knex",
  "version": "0.1.3",
  "description": "Express boilerplate with Knex.js",
  "main": "index.js",
  "scripts": {
    "knex": "knex",
    "dev": "nodemon index --exec \"babel-node index --presets es2015,stage-0\"",
    "start": "babel-node index --presets es2015,stage-0",
    "start_all": "(npm run start) & (npm run menu:update) & (npm run sync:feedback)",
    "start:dev": "nodemon",
    "menu:update": "babel-node menu_update --presets es2015,stage-0",
    "sync:feedback": "babel-node sync_sms --presets es2015,stage-0",
    "start:performance:sms": "babel-node performanceSMS --presets es2015,stage-0",
    "test": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-academy-challenges/boilerplate-knex.git"
  },
  "author": "EDA",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dev-academy-challenges/boilerplate-knex/issues"
  },
  "homepage": "https://github.com/dev-academy-challenges/boilerplate-knex#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "commitizen": "^2.10.1",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "cz-conventional-changelog": "^2.1.0",
    "dhis2-uid": "^0.1.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "knex": "^0.14.2",
    "make-fetch-happen": "^3.0.0",
    "morgan": "^1.8.0",
    "node-fetch": "^2.1.2",
    "nodemailer": "^6.4.5",
    "pg": "^7.18.2",
    "r2": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "cheerio": "^1.0.0-rc.2",
    "eslint": "^3.1.1",
    "jest": "^22.1.4",
    "nodemon": "^1.14.11",
    "supertest": "^3.0.0"
  }
}
